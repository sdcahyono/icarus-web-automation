  "scripts": {
    "test": "wdio run ./wdio.conf.js --spec %npm_config_feature%" ,
    "test-tag": "wdio run ./wdio.conf.js  --spec ./features/*.feature --cucumberOpts.tags=\"$npm_config_tag\"",
    "wdio": "wdio run ./wdio.conf.js",
    "report": "allure generate ./allure-results --clean && allure open",
    "test-all": "wdio run ./wdio.conf.js",
    "test-login": "wdio run ./wdio.conf.js  --suite login",
    "test-cart": "wdio run ./wdio.conf.js  --suite cart",
    "test-suite": "wdio run ./wdio.conf.js --suite %npm_config_dir%",
    "test-suite-preprod": "wdio run ./wdio-preprod.conf.js --suite %npm_config_dir%",
    "test-suite-firefox": "wdio run ./wdio-firefox.conf.js --suite %npm_config_dir%",
    "test-suite-firefox-preprod": "wdio run ./wdio-firefox-preprod.conf.js --suite %npm_config_dir%",
    "url-preprod": "set BASE_URL=https://uat-internal.thebodyshop.co.id/",
    "url-staging": "set BASE_URL=https://web.staging-v1.tbsgroup.co.id/",
    "test:staging": "NODE_ENV=staging wdio wdio.conf.js",
    "test:preprod": "NODE_ENV=preprod wdio wdio.conf.js"
  },
  "name": "icarus-web-automation",
  "description": "run the test with the following command\r npm run test",
